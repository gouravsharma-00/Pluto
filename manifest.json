{
    "manifest_version" : 3,
    "name" : "Pluto",
    "version" : "1.0.0",
    "description" : 
    "all-in-one productivity companion, built right into your browser. Stay organized, focused, and on track as you orbit through your daily life",
    "icons": {
        "16": "public/icons/icon16.png",
        "32": "public/icons/icon32.png"
    },
    "background" : {
        "service_worker" : "app/service/service-worker.js",
        "type" : "module"
    },
    "action" : {},
    "permissions" : [
        "scripting"
    ],
    "host_permissions" : [
        "http://*/*",
        "https://*/*"
    ]

}